; abstract method ,do something on the frame
(define (wave pic frame origin-op edge1-op edg2-op)
	(let ( newframe (make-frame (origin-op (origin-frame frame))
		(edg1-op (edge1-frame frame))
		(edg2-op (edge2-frame frame))))
		(paint pic frame)))
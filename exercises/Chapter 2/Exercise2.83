(define (raise param)
	(let ((supertype (nextlevel (type-tag param))))
		(let (this->super (get-coercion (type-tag param) nextlevel))
			(if this-super (this-super param)
				(error "no supertype defined")))))
(define (nextlevel type)
	(cond ((eq? type 'integer) 'rational)
		  ((eq? type 'rational) 'real)
		  ((eq? type 'real) 'complex)
		  (else (error "no supertype of" type))))